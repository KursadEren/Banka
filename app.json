{
  "expo": {
    "name": "Banka",
    "slug": "Banka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "expo": {
          "packagerOpts": {
            "port": 19001
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "expo": {
          "packagerOpts": {
            "port": 19000
          },
          "assetBundlePatterns": [
            "assets/fonts/Futura.ttf"
          ]
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
